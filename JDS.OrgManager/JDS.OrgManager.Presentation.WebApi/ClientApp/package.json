{
    "name": "org-manager",
    "version": "1.0.0",
    "license": "",
    "scripts": {
        "ng": "ng",
        "start": "echo hello & ng serve",
        "start:prod": "npm run build:prod && npm run server",
        "build": "ng build",
        "build:prod": "ng build --prod --aot",
        "lint": "eslint --color -c .eslintrc --ext .ts .",
        "test": "npm run lint && ng test --configuration=test",
        "watch": "ng test --configuration=test --browsers ChromeHeadless --watch --reporters dots",
        "e2e": "ng e2e",
        "e2e:ci": "ng e2e",
        "ci": "npm run format:test && npm run lint && ng test --configuration=test --browsers ChromeTravisCi --code-coverage && npm run build:prod -- --deploy-url / --base-href /",
        "format:write": "npx prettier **/*.{ts,json,md,scss} --write",
        "format:test": "npx prettier **/*.{ts,json,md,scss} --list-different",
        "release": "standard-version && git push --follow-tags origin master",
        "analyze": "npm run build:prod -- --stats-json && webpack-bundle-analyzer ./dist/org-manager/stats-es2015.json"
    },
    "private": true,
    "dependencies": {
        "@angular-extensions/elements": "~11.0.1",
        "@angular-extensions/model": "^10.0.1",
        "@angular/animations": "~11.2.0",
        "@angular/cdk": "~11.2.0",
        "@angular/common": "~11.2.0",
        "@angular/compiler": "~11.2.0",
        "@angular/core": "~11.2.0",
        "@angular/forms": "~11.2.0",
        "@angular/material": "~11.2.0",
        "@angular/platform-browser": "~11.2.0",
        "@angular/platform-browser-dynamic": "~11.2.0",
        "@angular/router": "~11.2.0",
        "@fortawesome/angular-fontawesome": "^0.8.2",
        "@fortawesome/fontawesome-free": "^5.15.2",
        "@fortawesome/fontawesome-svg-core": "^1.2.34",
        "@fortawesome/free-brands-svg-icons": "^5.15.2",
        "@fortawesome/free-solid-svg-icons": "^5.15.2",
        "@ngrx/effects": "~11.0.0",
        "@ngrx/entity": "~11.0.0",
        "@ngrx/router-store": "~11.0.0",
        "@ngrx/store": "~11.0.0",
        "@ngrx/store-devtools": "~11.0.0",
        "@ngx-translate/core": "^13.0.0",
        "@ngx-translate/http-loader": "^6.0.0",
        "oidc-client": "1.11.4",
        "bootstrap": "^4.6.0",
        "browser-detect": "^0.2.28",
        "rxjs": "~6.6.3",
        "tslib": "^2.1.0",
        "uuid": "^8.3.2",
        "zone.js": "~0.11.3"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.1102.0",
        "@angular-eslint/eslint-plugin": "^1.2.0",
        "@angular/cli": "~11.2.0",
        "@angular/compiler-cli": "~11.2.0",
        "@angular/language-service": "~11.2.0",
        "@types/jasmine": "~3.6.3",
        "@types/jasminewd2": "^2.0.8",
        "@types/node": "^14.14.28",
        "@types/uuid": "^8.3.0",
        "@typescript-eslint/eslint-plugin": "^4.15.0",
        "@typescript-eslint/eslint-plugin-tslint": "^4.15.0",
        "@typescript-eslint/parser": "^4.15.0",
        "all-contributors-cli": "^6.20.0",
        "codelyzer": "^6.0.1",
        "eslint": "^7.20.0",
        "eslint-config-prettier": "^7.2.0",
        "eslint-plugin-import": "^2.22.1",
        "express": "^4.17.1",
        "jasmine-core": "~3.6.0",
        "jasmine-spec-reporter": "~6.0.0",
        "karma": "~6.1.1",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage-istanbul-reporter": "~3.0.3",
        "karma-jasmine": "~4.0.1",
        "karma-jasmine-html-reporter": "^1.5.4",
        "karma-spec-reporter": "^0.0.32",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.2.1",
        "pretty-quick": "^3.1.0",
        "protractor": "^7.0.0",
        "rimraf": "^3.0.2",
        "standard-version": "^9.1.0",
        "ts-node": "~9.1.1",
        "tslint": "~6.1.3",
        "typescript": "~4.1.5",
        "webpack-bundle-analyzer": "^4.4.0"
    }
}
